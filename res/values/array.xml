<!-- Array resources for the Phone app. -->
<resources>
    <string-array translatable="false" name="transport_types">
        <item>UDP</item>
        <item>TCP</item>
    </string-array>

    <string-array translatable="true" name="sip_call_options_entries">
        <item>@string/sip_call_options_entry_1</item>
        <item>@string/sip_call_options_entry_2</item>
        <item>@string/sip_call_options_entry_3</item>
    </string-array>

    <string-array translatable="true" name="sip_call_options_wifi_only_entries">
        <item>@string/sip_call_options_wifi_only_entry_1</item>
        <item>@string/sip_call_options_entry_2</item>
        <item>@string/sip_call_options_entry_3</item>
    </string-array>

    <string-array translatable="false" name="sip_call_options_values">
        <item>@string/sip_always</item>
        <item>@string/sip_address_only</item>
        <item>@string/sip_ask_me_each_time</item>
    </string-array>

    <string-array translatable="false" name="phone_type_values">
        <item>@string/pstn_phone</item>
        <item>@string/internet_phone</item>
    </string-array>

    <string-array translatable="true" name="sip_send_keepalive_options">
        <item>@string/sip_system_decide</item>
        <item>@string/sip_always_send_keepalive</item>
    </string-array>

    <!-- default sim for data connection options. -->
    <string-array name="default_sim_for_data_connection_entries">
        <item>SIM 1 as default</item>
        <item>SIM 2 as default</item>
    </string-array>

    <!-- Values for default_sim_for_data_connection_entries matching constants in network_settings. Do not translate. -->
    <string-array name="default_sim_for_data_connection_values" translatable="false">
        <item>0</item>
        <item>1</item>
    </string-array>


    <!-- sim1 absent data connection options. -->
    <string-array name="sim1_absent_data_connection_entries">
        <item>SIM 2 as default</item>
    </string-array>

    <!-- Values for sim1_absent_data_connection_entries matching constants in network_settings. Do not translate. -->
    <string-array name="sim1_absent_data_connection_values" translatable="false">
        <item>1</item>
    </string-array>

    <!-- sim2 absent data connection options. -->
    <string-array name="sim2_absent_data_connection_entries">
        <item>SIM 1 as default</item>
    </string-array>

    <!-- Values for sim2_absent_data_connection_entries matching constants in network_settings. Do not translate. -->
    <string-array name="sim2_absent_data_connection_values" translatable="false">
        <item>0</item>
    </string-array>

    <string-array name="default_sim_for_3g_network_entries">
        <item>SIM1 is 3G prefer</item>
        <item>SIM2 is 3G prefer</item>
        <item>Both SIMs are 2G</item>
    </string-array>

    <string-array name="default_sim_for_3g_network_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>
    
    <string-array name="dual_3g_network_entries">
        <item>SIM1 is 3G prefer</item>
        <item>SIM2 is 3G prefer</item>
        <item>Both SIMs are 2G</item>
        <item>Both SIMs are 3G</item>
    </string-array>

    <string-array name="dual_3g_network_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
    </string-array>

    <string-array name="sim1_absent_3g_network_entries">
        <item>SIM2 is 3G prefer</item>
        <item>SIM2 is 2G</item>
    </string-array>

    <string-array name="sim1_absent_3g_network_values" translatable="false">
        <item>1</item>
        <item>2</item>
    </string-array>
    
    <string-array name="sim2_absent_3g_network_entries">
        <item>SIM1 is 3G prefer</item>
        <item>SIM1 is 2G</item>
    </string-array>

    <string-array name="sim2_absent_3g_network_values" translatable="false">
        <item>0</item>
        <item>2</item>
    </string-array>
</resources>
