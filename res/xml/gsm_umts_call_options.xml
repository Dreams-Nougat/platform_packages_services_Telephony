<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:telephony="http://schemas.android.com/apk/res/com.android.telephony"
     android:title="@string/labelGSMMore">

    <PreferenceScreen
        android:key="button_cf_expand_key"
        android:title="@string/labelCF"
        android:persistent="false">

        <intent android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.telephony.services"
            android:targetClass="com.android.telephony.services.GsmUmtsCallForwardOptions"/>
    </PreferenceScreen>

    <PreferenceScreen
        android:key="button_more_expand_key"
        android:title="@string/additional_gsm_call_settings"
        android:persistent="false">

        <intent android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.telephonyservices"
            android:targetClass="com.android.telephony.services.GsmUmtsAdditionalCallOptions"/>
    </PreferenceScreen>
</PreferenceScreen>
